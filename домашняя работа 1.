1 задание 
n=int(input())
k=int(input())
c=k//n
print(c,k-(n*c))

2 задание 
a=int(input())
b=str(a//100)
c=str(a//10%10)
d=str(a%10)
print(b+c+d,b+d+c,c+d+b,c+b+d,d+c+b,d+b+c,sep='\n')

3 задание
a=int(input())
b=int(input())
c=int(input())
d=a+b+c
k=d//60+10
s=d%60
print(k,':',s)

задание 4
a=int(input())
b=int(input())
if a==b:
    print('пароль принят!')
else:
    print('пароль не принят! введенные данные не совпадают')
    
    
  задание 5
    a=int(input())
b=int(input())
